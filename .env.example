# Example .env configuration for Universal MCP Server

# Flowise Backend Configuration
FLOWISE_ENABLED=true
FLOWISE_API_URL=http://localhost:3000
FLOWISE_API_KEY=

# Langflow Backend Configuration
LANGFLOW_ENABLED=true
LANGFLOW_API_URL=http://localhost:7860
LANGFLOW_API_KEY=

# Redis State Persistence (Optional)
REDIS_STATE_ENABLED=true
REDIS_SESSION_TTL_SECONDS=604800
REDIS_EXECUTION_TTL_SECONDS=86400
REDIS_KEY_PREFIX=agentic_flywheel
REDIS_HOST=localhost
REDIS_PORT=6379

# Langfuse Tracing (Optional)
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com
AGENTIC_FLYWHEEL_PARENT_TRACE_ID=
